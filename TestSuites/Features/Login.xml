<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Login">
    <listeners>
        <listener class-name="listeners.ITestListener_Listener"/>
        <listener class-name="listeners.IRetryAnalyzer_Listener"/>
    </listeners>

    <parameter name="browser" value="chrome"/>
    <parameter name="url" value="https://agentportalsit.axispay.app:444/login?redirect_url=%2Fwallet%2Fregistration"/>

    <test name="Login w/ Valid and Invalid Credentials">
        <classes>
            <class name="tests.login_tests.Login_Test"/>
        </classes>
    </test>
</suite>